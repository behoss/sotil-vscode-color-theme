  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#66AA66",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.function",
        "meta.function",
        "meta.function-call",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other",
        "storage.type",
        "storage.modifier",
        "support.type.sys-types",
        "support.type.posix-reserved",
        "support.type.stdint",
        "constant.language",
        "keyword.operator",
        "keyword.operator.logical",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.new",
        "punctuation.separator",
        "support.variable.glsl"
      ],
      "settings": {
        "foreground": "#999999",
        "fontStyle": ""
      }
    },
    {
      "scope": ["string", "constant.numeric"],
      "settings": {
        "foreground": "#AAAADD",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "meta.preprocessor.include",
        "meta.preprocessor.macro",
        "keyword.control.directive.conditional",
        "keyword.control.directive.include",
        "keyword.control.directive.define",
        "keyword.control.directive.undef",
        "constant.character.escape.line-continuation",
        "keyword.control.directive.pragma",
        "meta.preprocessor.pragma",
        "keyword.control.hlsl",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#7777AA",
        "fontStyle": ""
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#EE3322",
        "fontStyle": ""
      }
    },
    {
      "scope": ["meta.paragraph.markdown"],
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#AAAAAA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#AAAAAA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.raw"],
      "settings": {
        "foreground": "#AAAADD",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "markup.list",
        "beginning.punctuation.definition.list.markdown"
      ],
      "settings": {
        "foreground": "#AAAAAA",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#BD7ACA",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "keyword.control.flow.return",
      "settings": {
        "foreground": "#DAA7E6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["storage.modifier", "keyword.other.class"],
      "settings": {
        "foreground": "#AF7EB9"
      }
    },
    {
      "scope": ["entity.name.function", "keyword.other.new"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.variable.parameter", "variable.other.readwrite"],
      "settings": {
        "foreground": "#8CBDD4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "foreground": "#E7E7E7"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.type"],
      "settings": {}
    },
    {
      "scope": ["variable.other.object.property"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#587055",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#CEB545"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
